{
  "name": "observable-entropy-worker",
  "description": "An API proxy for the Truestamp Observable Entropy project.",
  "author": "Truestamp Inc. <support@truestamp.com>",
  "repository": "https://github.com/truestamp/observable-entropy-worker",
  "license": "UNLICENSED",
  "private": true,
  "version": "1.0.0",
  "main": "dist/worker.js",
  "scripts": {
    "build": "npm run lint && webpack",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "test": "jest --config jestconfig.json --verbose",
    "wrangler-update": "npm uninstall -g @cloudflare/wrangler && npm install -g @cloudflare/wrangler",
    "deploy": "wrangler publish"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "dependencies": {
    "@stablelib/hex": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "worktop": "^0.7.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.5.1",
    "luxon": "^2.3.1",
    "prettier": "^2.6.2",
    "superstruct": "^0.15.4",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
